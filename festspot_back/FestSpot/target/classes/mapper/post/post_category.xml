<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.festspot.dev.domain.postCategory.PostCategoryMapper">

    <select id="findeByCategoryKey"
            resultType="com.festspot.dev.domain.postCategory.PostCategory">
        select
            *
        from
            post_category_tb
        where
            post_category_key = #{postCategoryKey}
    </select>
    <select id="findAll" resultType="com.festspot.dev.domain.postCategory.PostCategory">
        select
            *
        from
            post_category_tb
    </select>
</mapper>