<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.festspot.dev.domain.performance.performanceRegion.PerformanceRegionMapper">
  <select id="findByCode"
          resultType="com.festspot.dev.domain.performance.performanceRegion.PerformanceRegion">
    select
      *
    from
      performance_region_tb
    where
      region_code = #{regionCode}
  </select>
  <select id="findByRegionName"
          resultType="com.festspot.dev.domain.performance.performanceRegion.PerformanceRegion">
    select
      *
    from
      performance_region_tb
    where
      region_name = #{regionName}
  </select>
</mapper>